{
  "name": "indo-foreign-surgico",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:dev": "ng serve --port=4200 --configuration='development' --poll=200 --proxy-config proxyconfig.json",
    "start:prod": "ng serve --port=4200 --configuration='production' --poll=200 --proxy-config proxyconfig.json",
    "build:dev": "ng build --watch --poll=200 --sourceMap-true --base-href=/",
    "build:prod":"ng build --aot --configuration='production' --watch --poll=200 --base-href=/",
    "analyze": "ng build --aot --configuration='production' --base-href=/ --stats-json && webpack-bundle-analyzer dist/indo-foreign-surgico/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.12",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^4.6.1",
    "jquery": "^3.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
